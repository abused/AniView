{
  "expo": {
    "name": "AniView",
    "slug": "AniView",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/white_logo.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#272C38"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "abusedmaster.aniview",
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6445224790923907~7486007502"
      }
    },
    "android": {
      "package": "abusedmaster.aniview",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6445224790923907~6603977158"
      }
    },
    "notification": {
      "iosDisplayInForeground": true
    }
  }
}
